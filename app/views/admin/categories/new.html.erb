<h1>New Category</h1>

<%= form_with(model: @category, url: admin_categories_path) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.submit "Create Category" %>
  </div>
<% end %>

<%= link_to "Back to Categories", admin_categories_path %>